{
	"name": "expo-app",
	"version": "1.0.0",
	"main": "index.js",
	"private": true,
	"scripts": {
		"lint": "eslint . --fix",
		"format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
		"start": "npx expo start -c",
